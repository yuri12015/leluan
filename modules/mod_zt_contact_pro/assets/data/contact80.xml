<contact version="1" xmlns="http://xspf.org/ns/0/">
					<elementList>
<param>

												<type>textfield</type>
<fieldtitle>Name</fieldtitle>

												<fieldname>name</fieldname>

												<valid>1</valid>
<size>30</size>

												<length>30</length>
</param>
<param>

												<type>textfield</type>
<fieldtitle>Email</fieldtitle>

												<fieldname>email</fieldname>

												<valid>1</valid>
<size>30</size>

												<length>30</length>
</param>
<param>

												<type>textfield</type>
<fieldtitle>Address</fieldtitle>

												<fieldname>address</fieldname>

												<valid>0</valid>
<size>30</size>

												<length>30</length>
</param>
<param>

												<type>textarea</type>
<fieldtitle>Message</fieldtitle>

												<fieldname>message</fieldname>

												<valid>0</valid>
<cols>30</cols>

											<rows>5</rows>
</param>
<param>

												<type>selected</type>
<fieldtitle>Choose Templates</fieldtitle>

												<fieldname>template</fieldname>

												<valid>1</valid>
<size>6</size>

												<multi>1</multi>
<value>JV Maju|JV Maju Template</value>
<value>JV Mira</value>
</param>
</elementList>
	 					</contact>